{
  "name": "bulkan",
  "version": "1.0.0",
  "description": "GeoJSON and TopoJSON data for PH volcanoes.",
  "main": "index.html",
  "scripts": {
    "build": "parcel build index.html -d public --no-source-maps",
    "prepare": "husky install",
    "start": "parcel index.html --open",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j4ckofalltrades/bulkan.git"
  },
  "author": "Jordan Duabe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j4ckofalltrades/bulkan/issues"
  },
  "types": "types",
  "homepage": "https://bulkan.vercel.app",
  "devDependencies": {
    "@mapbox/geojson-merge": "^1.1.1",
    "@types/geojson": "7946.0.8",
    "@types/topojson": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "maplibre-gl": "^2.1.9",
    "parcel-bundler": "^1.3.1",
    "prettier": "^2.6.2",
    "topojson-server": "^3.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vercel": "^24.2.4"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false
  },
  "lint-staged": {
    "*.{geojson,topojson}": "prettier --write data"
  }
}
